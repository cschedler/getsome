<h1>New Movements</h1>

<%= form_with scope: :movement, index: @workouts.movement_number.count, url: movements_path, local: true do |t| %>
	<p>
		<%= form.label :movement %>
		<%= form.label :set %>
		<%= form.label :rep %>
		<%= form.label :weight %>
		<%= form.label :duration %>
	</p>

	<p>
		<%= form.text_field :movement %>
		<%= form.text_field :set %>
		<%= form.text_field :rep %>
		<%= form.text_field :weight %>
		<%= form.text_field :duration %>
	</p>
<% end %>
