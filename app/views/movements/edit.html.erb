<h1>Edit Movement</h1>

<%= form_for ([@workout, @movement]) do |form| %>
	<p>
		<%= form.label :movement %>
		<%= form.label :set %>
		<%= form.label :rep %>
		<%= form.label :weight %>
		<%= form.label :duration %>
	</p>

	<p>
		<%= form.text_field :movement %>
		<%= form.text_field :set %>
		<%= form.text_field :rep %>
		<%= form.text_field :weight %>
		<%= form.text_field :duration %>
	</p>

	<p>
	<%= form.submit %>
	</p>

<% end %>

<%= link_to'Back', user_workout_path(@workout) %>
