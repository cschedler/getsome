<h1>Edit Movement</h1>

<%= form_with(@workout, Movement.create) do |form| %>
	<p>
		<%= form.label :movement %>
		<%= form.label :set %>
		<%= form.label :rep %>
		<%= form.label :weight %>
		<%= form.label :duration %>
	</p>

	<p>
		<%= form.text_field :movement %>
		<%= form.text_field :set %>
		<%= form.text_field :rep %>
		<%= form.text_field :weight %>
		<%= form.text_field :duration %>
	</p>
<% end %>

<% link_to'Back', edit_workouts_path %>
